DEBUG=false # true for debugger mode in local

CURE_SCHEDULE_WINDOW_MINUTES=180

LOGGING_LEVEL=debug # info and error available

GRPC_LISTEN_ADDRESS=50051
HTTP_LISTEN_ADDRESS=8080

POSTGRES_HOST=localhost
